{
  "id": "6d9e8e26-38fb-49a8-9478-cc89a9569129",
  "name": "\ud83d\udd0d Website Scraper: Auto-Generate Product Catalog + Static Pages",
  "description": "Drop in any website URL and let AgenticFlow AI do the heavy lifting. This workflow instantly extracts up to 20 product catalog items (with names, prices, and descriptions) and 10 static content pages (like About, FAQ, or Contact) \u2014 perfect for site audits, product research, or competitive analysis.",
  "created_at": "2025-02-14T08:53:40.293445",
  "updated_at": "2025-05-09T02:58:19.960800",
  "user_id": "64892703cf797ba34960a8f0",
  "nodes": {
    "nodes": [
      {
        "name": "firecrawl_map",
        "title": "Firecrawl Map",
        "description": "Attempts to output all website's urls in a few seconds.",
        "node_type_name": "firecrawl_map",
        "input_config": {
          "url": "{{url}}",
          "limit": 1000,
          "search": null,
          "ignore_sitemap": true,
          "include_subdomains": false
        },
        "output_mapping": null,
        "connection": "{{__app_connections__['a697bf0d-ec25-401c-ba80-0e5e71d5025a']}}",
        "cost": null,
        "metadata": null
      },
      {
        "name": "google_gen_ai_ask_gemini",
        "title": "Ask Gemini",
        "description": "Ask a Gemini anything you want!",
        "node_type_name": "google_gen_ai_ask_gemini",
        "input_config": {
          "model": "gemini-2.0-flash",
          "top_p": 1,
          "images": null,
          "prompt": "\"{{firecrawl_map.result}}\"",
          "max_tokens": 2048,
          "temperature": 0.9,
          "system_message": "You are an expert URL analyzer. Based on the provided list of URLs, please analyze and return:\n1. Top 20 unique product URLs. Look for patterns in URL structure that indicate:\n- Best-selling or popular products\n- Featured products\n- Products from different categories to ensure variety\nMake sure to mix the order of URLs to provide a diverse selection.\n2. Maximum 10 static pages related to FAQs or website information\nPlease analyze the URL patterns, paths, and keywords to identify the most relevant pages in each category.",
          "response_schema": {
            "type": "object",
            "title": "URLAnalysisSchema",
            "required": [
              "best_selling_products",
              "static_pages"
            ],
            "properties": {
              "static_pages": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "title": "Static Pages"
              },
              "best_selling_products": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "title": "Best Selling Products"
              }
            }
          },
          "model_response_modalities": []
        },
        "output_mapping": null,
        "connection": "{{__app_connections__['6c3b4c6d-27b5-405e-aeed-7134e459628b']}}",
        "cost": null,
        "metadata": null
      },
      {
        "name": "string_to_json",
        "title": "Convert String to JSON",
        "description": "Convert a string representation to a JSON object.",
        "node_type_name": "string_to_json",
        "input_config": {
          "string_to_convert": "{{google_gen_ai_ask_gemini.content.content.parts[0].text}}"
        },
        "output_mapping": null,
        "connection": null,
        "cost": null,
        "metadata": null
      },
      {
        "name": "firecrawl_extract_products",
        "title": "Firecrawl Extract (Products)",
        "description": "Extract data from a website using Firecrawl.",
        "node_type_name": "firecrawl_extract",
        "input_config": {
          "urls": "{{string_to_json.json_output.best_selling_products}}",
          "prompt": "Extract the product information from the list of URLs",
          "schema": {
            "type": "object",
            "title": "ProductListSchema",
            "required": [
              "products"
            ],
            "properties": {
              "products": {
                "type": "array",
                "items": {
                  "type": "object",
                  "title": "ProductSchema",
                  "required": [
                    "id",
                    "name",
                    "description",
                    "price",
                    "price_currency",
                    "image_url",
                    "product_url"
                  ],
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "price": {
                      "type": "number"
                    },
                    "image_url": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "product_url": {
                      "type": "string"
                    },
                    "price_currency": {
                      "type": "string",
                      "description": "Currency code in ISO 4217 format (e.g., USD, EUR, VND, GBP)"
                    }
                  }
                }
              }
            }
          },
          "enable_web_search": false
        },
        "output_mapping": null,
        "connection": "{{__app_connections__['a697bf0d-ec25-401c-ba80-0e5e71d5025a']}}",
        "cost": null,
        "metadata": null
      },
      {
